{
  "name": "rsvp-db",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf ./dist && tsc --outDir ./dist",
    "permission:wait-for": "chmod +x ./scripts/wait-for.sh",
    "migrate:make": "knex --knexfile ./knexfile.ts migrate:make -x ts",
